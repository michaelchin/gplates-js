{
  "name": "gplates",
  "version": "0.0.1",
  "description": "GPlates NPM package",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "type": "module",
  "scripts": {
    "test": "node --no-warnings ./dist/test/main.mjs && node --no-warnings ./src/test/test-use-require.cjs",
    "compile:mjs": "./node_modules/.bin/babel src --out-dir dist --extensions '.ts' --out-file-extension '.mjs'",
    "compile:cjs": "./node_modules/.bin/babel src --out-dir dist --extensions '.ts' --out-file-extension '.cjs' --config-file ./babel.config.compat.cjs",
    "compile": "npm run compile:mjs && npm run compile:cjs && tsc --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelchin/gplates-js.git"
  },
  "keywords": [
    "gplates",
    "tectonic"
  ],
  "author": "Michael Chin",
  "license": "GPL-2.0-only",
  "bugs": {
    "url": "https://github.com/michaelchin/gplates-js/issues"
  },
  "homepage": "https://github.com/michaelchin/gplates-js#readme",
  "dependencies": {
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/node": "^18.15.11",
    "babel-plugin-add-import-extension": "^1.6.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
